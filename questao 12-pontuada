#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int numero;
    int soma = 0;
    int media = 0;
    int contador;
    
    for(i = 1;i <= 5;i++){
     printf("Informe o %dº número:",i);
     scanf("%i",&numero);
     
     contador++;
     soma += numero;
     media = soma / contador;
    }
    printf("MÉDIA: %i \n",media);
    return 0;
}
