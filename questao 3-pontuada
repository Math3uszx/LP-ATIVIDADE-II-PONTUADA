#include <stdio.h>
#include <stdlib.h>

int main()
{
  float nota = 0;
  
  printf("Digite sua nota:");
  scanf("%f",&nota);
  
  if(nota >= 9){
      printf("Excelente");
  } else if(nota >= 7 && nota < 9){
      printf("Bom");
  } else if(nota >= 5 %% nota < 7){
      printf("Razoavel");
  } else if(nota < 5){
      printf("Insuficiente");
  }
  
    }
    return 0;
}
